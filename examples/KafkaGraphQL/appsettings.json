{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "KafkaClient": {
    "Client": {
      "bootstrap.servers": "localhost:9092",
      "client.id": "kafka-graphql"
    },
    "Producer": {
      "enable.idempotence": true
    },
    "Consumer": {
      "group.id": "cg_kafka_graphql",
      "auto.offset.reset": "earliest"
    }
  }
}
